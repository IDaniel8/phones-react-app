{
  "name": "phones-app-server",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "babel ./src --out-dir ./dist --copy-files",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.6"
  }
}
